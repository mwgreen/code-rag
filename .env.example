# Milvus Configuration
MILVUS_DB_PATH=./data/milvus.db

# File Extensions to Index (comma-separated)
DEFAULT_EXTENSIONS=.java,.js,.ts,.jsx,.tsx,.json,.xml,.yaml,.yml,.md,.properties,.gradle,.html

# Directories to Exclude (comma-separated)
EXCLUDE_DIRS=node_modules,__pycache__,.git,venv,.venv,target,build,dist,.idea,.vscode

# Chunk Size Configuration
MAX_CHUNK_SIZE=2000
MIN_CHUNK_SIZE=100
